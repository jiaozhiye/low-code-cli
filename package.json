{
  "name": "vue3-cli",
  "version": "1.0.0",
  "author": "jiaozhiye",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development ENV_CONFIG=dev node --max-old-space-size=4096 build/dev.js",
    "dev": "cross-env NODE_ENV=development ENV_CONFIG=dev webpack serve --progress --config build/webpack.dev.conf.js",
    "gray": "cross-env NODE_ENV=development ENV_CONFIG=gray webpack serve --progress --config build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production ENV_CONFIG=prod node --max-old-space-size=32768 build/prod.js",
    "build-dev": "cross-env NODE_ENV=production ENV_CONFIG=dev node --max-old-space-size=32768 build/prod.js",
    "build-tst": "cross-env NODE_ENV=production ENV_CONFIG=tst node --max-old-space-size=32768 build/prod.js",
    "build-uat": "cross-env NODE_ENV=production ENV_CONFIG=uat node --max-old-space-size=32768 build/prod.js",
    "build-pre": "cross-env NODE_ENV=production ENV_CONFIG=pre node --max-old-space-size=32768 build/prod.js",
    "build-gray": "cross-env NODE_ENV=production ENV_CONFIG=gray node --max-old-space-size=32768 build/prod.js",
    "lint": "eslint ./src --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "format": "prettier --write \"./src/**/*.{vue,js,ts}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,jsx,tsx,vue}": [
      "npm run lint-fix",
      "git add"
    ],
    "src/**/*.{scss}": "prettier --write"
  },
  "dependencies": {
    "@jiaozhiye/qm-design": "^1.1.0-beta.20",
    "add-dom-event-listener": "^1.1.0",
    "axios": "^0.21.4",
    "crypto-js": "^4.1.0",
    "dayjs": "^1.10.6",
    "js-cookie": "^3.0.0",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "omit.js": "^2.0.2",
    "screenfull": "^5.1.0",
    "vue": "^3.2.18",
    "vue-i18n": "^9.2.0-beta.4",
    "vue-router": "^4.0.11",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.14.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.14.0",
    "@babel/plugin-transform-runtime": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@vue/babel-plugin-jsx": "^1.1.0",
    "@vue/compiler-sfc": "^3.2.18",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^8.0.0",
    "copy-webpack-plugin": "^5.1.2",
    "core-js": "^3.14.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "dotenv-webpack": "^7.0.2",
    "eslint": "^7.15.0",
    "eslint-plugin-vue": "^7.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "html-webpack-plugin": "^5.3.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.2",
    "mini-css-extract-plugin": "^2.2.2",
    "ora": "^3.4.0",
    "postcss-loader": "^6.1.0",
    "prettier": "^2.2.0",
    "rimraf": "^2.7.0",
    "sass": "^1.34.0",
    "sass-loader": "^12.1.0",
    "style-resources-loader": "^1.4.1",
    "terser-webpack-plugin": "^5.1.2",
    "thread-loader": "^3.0.4",
    "ts-loader": "^9.2.2",
    "typescript": "^4.3.2",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "^16.8.1",
    "vue-style-loader": "^4.1.3",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-custom-theme": "^1.0.10",
    "webpack-dev-server": "^4.2.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 14.16.0",
    "npm": ">= 6.14.0"
  }
}
